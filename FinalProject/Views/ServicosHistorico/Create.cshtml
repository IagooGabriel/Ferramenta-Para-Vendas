@model FinalProject.Models.View.ServicoHistoricoModelView

@{
    ViewData["Title"] = "Criar*";
}

<h1>Criar</h1>

<h4>Histórico de Serviço</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="oqfoialterado" class="control-label"></label>
                <input asp-for="oqfoialterado" class="form-control" />
                <span asp-validation-for="oqfoialterado" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="servicosId" class="control-label">Servico: </label>
                <select asp-for="servicosId" asp-items="@ViewBag.ListaServicos"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Criar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voltar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
